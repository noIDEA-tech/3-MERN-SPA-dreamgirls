{
  "name": "3-mern-spa-dreamgirls",
  "version": "1.0.0",
  "description": "A MERN stack application for reporting and viewing safety concerns in your area",
  "main": "server/src/server.ts",
  "scripts": {
    "start": "node server/dist/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build && cd ../server && npm run build",
    "seed": "cd server && npm run seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noIDEA-tech/3-MERN-SPA-dreamgirls.git"
  },
  "keywords": [
    "safety",
    "community",
    "map",
    "mern",
    "typescript",
    "graphql"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/noIDEA-tech/3-MERN-SPA-dreamgirls/issues"
  },
  "homepage": "https://github.com/noIDEA-tech/3-MERN-SPA-dreamgirls#readme",
  "dependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
